{
    "name": "zchlm/myfavicon",
    "description": "CraftCMS plugin generating favicon.",
    "type": "craft-plugin",
    "version": "1.1.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "favicon"
    ],
    "support": {
        "docs": "https://github.com/zchlm/myfavicon/blob/master/README.md",
        "issues": "https://github.com/zchlm/myfavicon/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Akagibi",
            "homepage": "https://www.akagibi.com"
        },
        {
            "name": "Zchlm"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "akagibi\\myfavicon\\": "src/"
        }
    },
    "extra": {
        "name": "My Favicon",
        "handle": "myfavicon",
        "changelogUrl": "https://raw.githubusercontent.com/zchlm/myfavicon/master/CHANGELOG.md",
        "class": "akagibi\\myfavicon\\MyFavicon"
    }
}
